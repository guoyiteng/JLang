{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "./playground/src/ConcurrentReflection.o",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [{"JAVA_HOME": "/usr/lib/jvm/jdk1.7.0_80"}],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "type": "gdb",
            "request": "launch",
            "name": "Launch Program",
            "target": "./playground/src/ConcurrentReflection.o",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText",
            "env": {"JAVA_HOME": "/usr/lib/jvm/jdk1.7.0_80"},
        }
    ]
}